{
    "apps": [
        {
            "name": "bte-trapi",
            "cwd": "packages/@biothings-explorer/bte-trapi",
            "script": "./src/server.js",
    	    "exec_mode": "cluster",
            "instances": 2,
            "autorestart": true,
            "instance_var": "INSTANCE_ID",
            "env":{
                "PORT": "3000",
                "LOG": "./log"
            },
            "env_prod": {
       	        "NODE_ENV": "production",
                "REDIS_HOST": "localhost",
                "REDIS_PORT": 6379
            },
            "env_prod_nocache": {
                "NODE_ENV": "production"
            },
            "env_dev": {
                "DEBUG": "biomedical-id-resolver,bte*"
            }
        }
    ]
}